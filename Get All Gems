function addKeysToBackpack(){let e=Boot.prototype.game._state._current.user.source;[3,4,10,11,17].forEach(t=>{let a=e.backpack.data.key.find(e=>e.ID===t);a?a.N+=1:e.backpack.data.key.push({ID:t,N:1})}),e.appearance.updated=!0,e.updated=!0,e.saveEnabled=!0,e.forceSaveCharacter(),e.appearanceChanged=!0,e.backpack.updated=!0}function showConfirmationScreen(){let e=document.createElement("div");e.style.cssText="position:fixed;top:50%;left:50%;transform:translate(-50%,-50%);padding:20px;background:#fff;border:1px solid #ccc;border-radius:5px;box-shadow:0 4px 8px rgba(0,0,0,0.2);z-index:1001;";let t=document.createElement("h3");t.textContent="Confirm Gem Addition",e.appendChild(t);let a=document.createElement("p");a.textContent="Are you sure you want to add the gems to your backpack?",e.appendChild(a);let o=document.createElement("button");o.textContent="Confirm",o.style.cssText="padding:10px 15px;background:green;color:white;border:none;border-radius:4px;cursor:pointer;margin-right:10px;",o.onclick=function(){addKeysToBackpack(),document.body.removeChild(e)},e.appendChild(o);let n=document.createElement("button");n.textContent="Cancel",n.style.cssText="padding:10px 15px;background:red;color:white;border:none;border-radius:4px;cursor:pointer;",n.onclick=function(){document.body.removeChild(e)},e.appendChild(n),document.body.appendChild(e)}Boot.prototype.game._state._current.user.source.backpack.updated=!0,showConfirmationScreen();
